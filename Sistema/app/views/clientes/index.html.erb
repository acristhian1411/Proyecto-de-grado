<p id="notice"><%= notice %></p>

<h1>Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>N cedula</th>
      <th>Cli telefono</th>
      <th>Limite credito</th>
      <th>Cli activo</th>
      <th>Ciudades</th>
      <th>Barrio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nombre %></td>
        <td><%= cliente.apellido %></td>
        <td><%= cliente.n_cedula %></td>
        <td><%= cliente.cli_telefono %></td>
        <td><%= cliente.limite_credito %></td>
        <td><%= cliente.cli_activo %></td>
        <td><%= cliente.ciudades %></td>
        <td><%= cliente.barrio %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
